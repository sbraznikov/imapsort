# imapsort configuration

imap:
    server: my.email.server
    ssl: True
    port: 993
    user: my_user_name
    password: my_password

jira:
    conditions:
        from: jira@jira.symmetrics.de
    actions:
        move: INBOX.Trash

zalando:
    conditions:
        from: newsletter@service3.zalando-lounge.de
    actions:
        move: INBOX.Trash

computerfutures:
    conditions:
        from: computerfutures.de
    actions:
        move: INBOX.Trash
